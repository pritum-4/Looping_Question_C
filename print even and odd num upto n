#include <stdio.h>

int main() {
    printf("Enter n\n");
    int n;
    scanf("%d",&n);
    printf("Even: ");
    for(int i=1;i<=n;i++)
    {
        if(i>>1<<1==i) //or (i%2==0)
        {
            printf("%d ",i);
        }
    }
    printf("\n");
    printf("Odd: ");
    for(int i=1;i<=n;i++)
    {
        if(i>>1<<1 != i) //or(i%2!=0)
        {
            printf("%d ",i);
        }
    }
    return 0;
}
