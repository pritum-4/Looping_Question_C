#include <stdio.h>

int main() {
    // GCD/HCF of two numbers
    printf("Enter the two numbers for finding GCD\n");
    int n1,n2;
    scanf("%d",&n1);
    scanf("%d",&n2);
    int gcd;
    for(int i=1;i<=n1 && i<=n2;i++)
    {
        if( n1%i==0 && n2%i==0)
        {
            gcd=i;
        }
    }
    printf(" gcd of %d,%d is %d",n1,n2,gcd);
    return 0;
}
