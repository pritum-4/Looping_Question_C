#include<stdio.h>
int main()
{
    int n1;
    printf("Enter a number: ");
    scanf("%d", &n1);
    printf("The number is %d\n", n1);
    int count=0;
    int n=n1;
    while (n>0)
    {
        count=count*10+(n%10);
        n=n/10;
    }
    printf("The reversed number is %d\n",count);
    if(count==n1)
    {
      printf("The number is pallindrome");
    }
    else{
      printf("The number is not pallindrome");
    }
    
    return 0;
}
